MoveTool.Name = "DeleteTool "
MoveTool.Parent = LT2GUI2Frame
MoveTool.BackgroundColor3 = Color3.new(0, 0, 0)
MoveTool.BorderColor3 = Color3.new(0, 0, 0)
MoveTool.Position =  UDim2.new(0, 3, 0, 210)
MoveTool.Size = UDim2.new(0, 165, 0, 20)
